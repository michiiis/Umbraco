@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.GossipColumns>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}



@{
	var selection = Umbraco.Content(Guid.Parse("2968248e-283b-4c7d-ae26-71d6e5f23247"))
    .Children()
    .Where(x => x.IsVisible())
    .OrderBy(x => x.CreateDate)
    .Take(10);
}
        
	        @foreach (var item in selection)
	        {
	        <a href="@item.Url()">
            <div>
                @item.Value("title");
            </div>
            
            @{
                var image = item.Value<IPublishedContent>("leadImage");
                }
        
            <div>
                <img src="@image.Url()" class="img-fluid" />
            </div>
            </a>
            }




